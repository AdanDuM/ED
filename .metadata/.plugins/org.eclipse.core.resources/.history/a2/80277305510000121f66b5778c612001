#ifndef PILHA_H_
#define PILHA_H_

namespace Pilha {

class Pilha {
public:
	Pilha();
	virtual ~Pilha();
	virtual int empilhar(int);
	virtual int desempilhar();
	virtual int mostrarPilha();
	virtual int limpar();
private:
	int topo;
	int MAXPILHA;
	int PILHACHEIA;
	int pilha[];
};

} /* namespace Pilha */
#endif /* PILHA_H_ */
